<h1>I AM NEW</h1>

<%= form_for @user, url:sign_up_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit "Sign up woman" %>

<% end %>
